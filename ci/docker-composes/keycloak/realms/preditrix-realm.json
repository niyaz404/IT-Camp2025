{
  "id": "preditrix",
  "realm": "preditrix",
  "enabled": true,

  "roles": {
    "realm": [
      { "name": "ADMIN" },
      { "name": "OPERATOR" }
    ]
  },

  "users": [
    {
      "username": "admin",
      "enabled": true,
      "email": "admin@example.com",
      "firstName": "Администратор",
      "lastName": "Системы",
      "attributes": {
        "patronymic": "Иванович"
      },
      "credentials": [
        {
          "type": "password",
          "value": "admin",
          "temporary": false
        }
      ],
      "realmRoles": ["ADMIN"]
    },
    {
      "username": "galievnr",
      "enabled": true,
      "email": "galievnr@gpn.ru",
      "firstName": "Нияз",
      "lastName": "Галиев",
      "attributes": {
        "patronymic": "Рафисович"
      },
      "credentials": [
        {
          "type": "password",
          "value": "galievnr",
          "temporary": false
        }
      ],
      "realmRoles": ["ADMIN", "OPERATOR"]
    },
    {
      "username": "service-account-data-manager-service",
      "enabled": true,
      "serviceAccountClientId": "data-manager-service",
      "realmRoles": [],
      "clientRoles": {
        "realm-management": ["view-users"]
      }
    }
  ],

  "clientScopes": [
    {
      "name": "webapi-audience",
      "description": "Adds webapi as audience",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true",
        "display.on.consent.screen": "false"
      },
      "protocolMappers": [
        {
          "name": "webapi-audience-mapper",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-audience-mapper",
          "consentRequired": false,
          "config": {
            "included.client.audience": "webapi",
            "id.token.claim": "true",
            "access.token.claim": "true"
          }
        }
      ]
    }
  ],

  "clients": [
    {
      "clientId": "frontend",
      "protocol": "openid-connect",
      "publicClient": true,
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": false,
      "redirectUris": [
        "http://localhost:3000/*",
        "http://frontend:3000/*",
        "http://192.168.1.131:3000/*",
        "http://192.168.1.131:3000/",
        "*"
      ],
      "webOrigins": [
        "http://localhost:3000",
        "http://frontend:3000",
        "http://192.168.1.131:3000/*",
        "http://192.168.1.131:3000/",
        "*"
      ],
      "attributes": {
        "pkce.code.challenge.method": "S256",
        "post.logout.redirect.uris": "http://localhost:3000/*##http://frontend:3000/*##http://192.168.1.131:3000/*##http://192.168.1.131:3000/##*"
      },
      "defaultClientScopes": [
        "web-origins",
        "role_list",
        "profile",
        "email",
        "webapi-audience"
      ],
      "optionalClientScopes": [
        "address",
        "phone",
        "offline_access",
        "microprofile-jwt"
      ],
      "protocolMappers": [
        {
          "name": "given name",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-property-mapper",
          "config": {
            "userinfo.token.claim": "true",
            "user.attribute": "firstName",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "given_name",
            "jsonType.label": "String"
          }
        },
        {
          "name": "family name",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-property-mapper",
          "config": {
            "userinfo.token.claim": "true",
            "user.attribute": "lastName",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "family_name",
            "jsonType.label": "String"
          }
        },
        {
          "name": "email",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-property-mapper",
          "config": {
            "userinfo.token.claim": "true",
            "user.attribute": "email",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "email",
            "jsonType.label": "String"
          }
        },
        {
          "name": "patronymic",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "config": {
            "userinfo.token.claim": "true",
            "user.attribute": "patronymic",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "patronymic",
            "jsonType.label": "String"
          }
        },
        {
          "name": "realm roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "consentRequired": false,
          "config": {
            "multivalued": "true",
            "userinfo.token.claim": "true",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "roles",
            "jsonType.label": "String"
          }
        }
      ]
    },
    {
      "clientId": "webapi",
      "protocol": "openid-connect",
      "bearerOnly": true,
      "authorizationServicesEnabled": false,
      "serviceAccountsEnabled": false
    },
    {
      "clientId": "data-manager-service",
      "protocol": "openid-connect",
      "publicClient": false,
      "bearerOnly": false,
      "serviceAccountsEnabled": true,
      "standardFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "authorizationServicesEnabled": false,
      "clientAuthenticatorType": "client-secret",
      "secret": "super-secret-here",
      "defaultClientScopes": [
        "web-origins",
        "role_list",
        "profile",
        "email"
      ]
    }
  ]
}
